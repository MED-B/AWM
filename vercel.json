{
  "version": 2,
  "builds": [
    {
      "src": "AWM/**",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8.5",
        "buildCommand": "./build_files.sh"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "AWM/wsgi.py"
    }
  ]
}
